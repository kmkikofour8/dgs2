img{
    width:100%;
}
th{
    background-color:tan;
    font-family: 'Droid Sans', Arial, Helvetica, sans-serif;
}
#starFill{
    color:gold;
    content: "\2605";
    
}
.panel-success>.panel-heading{
   background-image:linear-gradient(to bottom,#734831 0,#734831 100%);
   color:white;
}
.item_container{
    -webkit-box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.4); 
-moz-box-shadow: 0px 1px 6px rgba(23, 69, 88, .5);

/* rounded corners */
-webkit-border-radius: 12px;
-moz-border-radius: 7px; 
border-radius: 7px;

}
.item_container1{
    
    -webkit-box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.4); 
-moz-box-shadow: 0px 1px 1px rgba(23, 69, 88, .5);
    
/* rounded corners */
-webkit-border-radius: 12px;
-moz-border-radius: 7px; 
border-radius: 7px;
}
.smallImage{
    width:93px;
    height:125px;
}
#pIcon{
    max-height: 20px;
    max-width:20px;
}
#propic{
    max-width:50px;
    max-height:50px;
}
.wrap{
   // min-height: 100%;
    height: auto;
    margin: 0 auto -250px;
    padding: 0 0 250px;
    margin-top: 20px;
}   
#navheader{
    margin-bottom:20px; 
}
hr{
    background-image:url('../img/hr.png');
    list-style : url('../img/hr.png') inside;
    border:0px;
    height:5px;
}
.container{
    color: black;
}
.navbar-default .navbar-nav>li>a {
    color: brown;
}
.dev_cell_wide {
    margin-bottom: 15px;
    padding: 10px;
    border: 1px solid #dddddd;
    border-radius: 4px;
}
.dev_cell_wide .image {
    padding: 5px;
    width: 100px;
    text-align: center;
}
.dev_cell_wide .body {
    padding-left: 100px;
}
.dev_cell_wide .title {
    font-size: 125%;
    font-weight: bold;
    text-align: left;
    width: 100%;
    height: 35px;
}
.dev_cell_wide .subtitle {
    font-size: 115%;
}
.dev_cell_wide .buttons {
    margin-top: 10px;
}
.btn {
    display: inline-block;
    margin-bottom: 0;
    font-weight: normal;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    white-space: nowrap;
    padding: 6px 12px;
    font-size: 13px;
    line-height: 1.42857143;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.panel{
     max-height: 171px;
}
